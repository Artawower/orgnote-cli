{
  "name": "second-brain-publisher",
  "version": "0.6.1",
  "description": "Project for synchronizing org roam notes and second brain ",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "start": "node --loader tsm src/index.ts",
    "clear": "rm -rf dist",
    "build": "npm run clear && tsc --project tsconfig.json && chmod +x ./dist/index.js",
    "prepublish": "npm run build",
    "convert:api": "fix-esm-import-path src/generated",
    "codegen:api": "openapi-generator-cli generate && yarn convert:api",
    "codegen:api2": "swagger-typescript-api --axios -p http://localhost:8000/v1/swagger/doc.json -o ./src -n generated/api.ts && yarn convert:api"
  },
  "repository": "git+https://github.com/Artawower/second-brain-publisher.git",
  "keywords": ["org", "roam", "second brain"],
  "author": "darkawower <artawower@protonmail.com> (https://du-blog.ru/)",
  "license": "MIT",
  "files": ["dist/**"],
  "bin": {
    "second-brain-publisher": "dist/index.js"
  },
  "bugs": {
    "url": "https://github.com/Artawower/second-brain-publisher/issues"
  },
  "homepage": "https://github.com/Artawower/second-brain-publisher#readme",
  "devDependencies": {
    "@digitak/tsc-esm": "3.1.4",
    "@openapitools/openapi-generator-cli": "2.7.0",
    "@types/axios": "0.14.0",
    "@types/jest": "27.4.1",
    "@types/node": "17.0.17",
    "@types/request": "2.48.8",
    "fix-esm-import-path": "1.4.0",
    "jest": "27.5.1",
    "nyc": "15.1.0",
    "prettier": "2.5.1",
    "swagger-axios-codegen": "0.15.11",
    "swagger-typescript-api": "13.0.1",
    "ts-jest": "27.1.3",
    "ts-node": "10.5.0",
    "tsm": "2.2.1",
    "typescript": "4.5.5"
  },
  "dependencies": {
    "@types/uuid": "^8.3.4",
    "axios": "0.26.1",
    "form-data": "4.0.0",
    "org-mode-ast": "0.2.8",
    "request": "2.88.2",
    "sqlite3": "5.1.6",
    "uuid": "8.3.2",
    "winston": "3.9.0",
    "yargs": "17.5.1",
    "zip-a-folder": "2.0.0"
  }
}
